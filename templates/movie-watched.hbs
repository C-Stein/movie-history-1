{{#each this}}
  <div class="watchedTemplate col-md-3">
    <div class="img-wrapper">
      <img class="media-object img-thumbnail" src="{{poster}}">
    </div>
    <div class="media-body">
      <p>{{rating}}</p> 
      <label for="ratingSystem">Rating System (1-10)</label>
        <input class="rating" data-max="10" data-min="1" id="some_id" name="ratingStar" type="number" />
      <div class="media-bottom">
        <button type="button" class="removeButton btn btn-default btn-sm">Remove</button>
      </div>
    </div>
  </div>
{{/each}}